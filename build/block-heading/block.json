{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wcb/heading",
  "version": "0.1.0",
  "title": "Heading",
  "category": "wcb-blocks",
  "icon": "heading - wcb-block-editor-block-icon",
